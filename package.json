{
    "name": "@hellostake/instagram",
    "version": "1.0.0",
    "description": "Capacitor plugin to download and share directly to instagram stories for iOS and share to different mediums for Android",
    "author": "Michael Oberwasserlechner",
    "license": "MIT",
    "main": "dist/esm/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rimraf ./dist",
        "watch": "tsc --watch",
        "test": "jest",
        "removePacked": "rimraf -g byteowls-capacitor-filesharer-*.tgz",
        "publishLocally": "npm run removePacked && npm run build && npm pack",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "android/src/main/",
        "android/build.gradle",
        "dist/",
        "ios/ByteowlsCapacitorFilesharer/Source",
        "HellostakeInstagram.podspec"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/stake-global/capacitor-instagram"
    },
    "keywords": [
        "capacitor",
        "capacitor-plugin",
        "share",
        "files",
        "filesharer"
    ],
    "capacitor": {
        "ios": {
            "src": "ios"
        },
        "android": {
            "src": "android"
        }
    },
    "peerDependencies": {
        "@capacitor/core": ">=5"
    },
    "dependencies": {
        "file-saver": "2.0.5"
    },
    "devDependencies": {
        "@capacitor/android": "5.3.0",
        "@capacitor/core": "5.3.0",
        "@capacitor/ios": "5.3.0",
        "@types/file-saver": "2.0.5",
        "@types/jest": "29.5.2",
        "eslint": "8.44.0",
        "jest": "29.6.0",
        "rimraf": "5.0.1",
        "ts-jest": "29.1.1",
        "typescript": "4.9.5"
    }
}
